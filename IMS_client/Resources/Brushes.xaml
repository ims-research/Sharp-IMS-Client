<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="TitlebarBackgroundBrush" Color="#FF292929"/>
    <SolidColorBrush x:Key="WindowForegroundBrush" Color="white"/>
    <SolidColorBrush x:Key="WindowBackgroundBrush" Color="#FF292929"/>

    <LinearGradientBrush x:Key="SemiTransparentOverlayBrush" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="#11FFFFFF"/>
        <GradientStop Offset="0.4" Color="#33FFFFFF"/>
        <GradientStop Offset="0.5" Color="#00FFFFFF"/>
    </LinearGradientBrush>
</ResourceDictionary>